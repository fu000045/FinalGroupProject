<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/linearlayout"
    tools:context="com.algonquin.finalgroupproject.MovieActivity">

    <ProgressBar
        android:id="@+id/indeterminateBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/TestViewFaveriate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Favorite" />

    <EditText
        android:id="@+id/EditTextInputFav"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="text" />
    <Button
        android:id="@+id/Movie1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Button_Movie1"
        tools:layout_editor_absoluteX="131dp"
        tools:layout_editor_absoluteY="181dp" />

    <Button
        android:id="@+id/Movie2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Button_Movie2"
        tools:layout_editor_absoluteX="131dp"
        tools:layout_editor_absoluteY="181dp" />
    <!--<RatingBar-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="match_parent" />-->
</LinearLayout>
